{
  "name": "mcp-generator",
  "version": "0.1.0",
  "description": "MCP Server Generator - Docker-in-Docker Auto Builder",
  "main": "dist/main.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "tsc --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,json,md}\""
  },
  "keywords": [
    "mcp",
    "server",
    "generator",
    "docker",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^20.10.6",
    "dockerode": "^4.0.2",
    "inquirer": "^9.2.12",
    "js-yaml": "^4.1.0",
    "typescript": "^5.3.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/dockerode": "^3.3.23",
    "@types/inquirer": "^9.0.7",
    "@types/js-yaml": "^4.0.9",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
