{
  "name": "mcp-generator",
  "version": "0.1.0",
  "description": "MCP Server Generator - Docker-in-Docker Auto Builder",
  "main": "dist/main.js",
  "bin": {
    "mcp-generator": "dist/cli.js",
    "mcp": "dist/cli.js"
  },
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "cli": "node dist/cli.js",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=jest-junit",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,json,md}\"",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch"
  },
  "keywords": [
    "mcp",
    "server",
    "generator",
    "docker",
    "automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^20.10.6",
    "@types/semver": "^7.7.1",
    "@types/tar-fs": "^2.0.4",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "dockerode": "^4.0.9",
    "inquirer": "^9.3.8",
    "js-yaml": "^4.1.1",
    "ora": "^9.0.0",
    "semver": "^7.7.3",
    "tar-fs": "^3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/dockerode": "^3.3.47",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/ora": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^30.2.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.4.5",
    "typedoc": "^0.28.14"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
